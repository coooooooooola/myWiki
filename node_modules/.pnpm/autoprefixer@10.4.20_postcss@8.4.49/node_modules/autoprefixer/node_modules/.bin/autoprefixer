#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.4.49/node_modules/autoprefixer/bin/node_modules:/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.4.49/node_modules/autoprefixer/node_modules:/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.4.49/node_modules:/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.4.49/node_modules/autoprefixer/bin/node_modules:/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.4.49/node_modules/autoprefixer/node_modules:/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.4.49/node_modules:/Users/swtywang/Coding/05 我的博客/my-docs-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/autoprefixer" "$@"
else
  exec node  "$basedir/../../bin/autoprefixer" "$@"
fi
